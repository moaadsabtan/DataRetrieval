{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=dotnetdataservice.db"
  },
  "Storage": {
    "FilePath": "./data/files",
    "FileRetentionMinutes": 30,
    "CacheMinutes": 10
  },
  "Redis": {
    "Enabled": false,
    "Configuration": "localhost:6379"
  }
}